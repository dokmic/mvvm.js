{
  "name": "ts-mvvm",
  "version": "0.1.0",
  "description": "Model-View-ViewModel Pattern Implementation for TypeScript",
  "keywords": [
    "mvvm",
    "ui",
    "typescript"
  ],
  "author": "Michael Dokolin <m@dokol.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dokmic/ts-mvvm"
  },
  "bugs": {
    "url": "https://github.com/dokmic/ts-mvvm/issues"
  },
  "main": "lib/index.ts",
  "scripts": {
    "test": "jest",
    "codecov": "codecov"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0",
    "codecov": "^3.0",
    "gts": "^0.6",
    "jest": "^23.0",
    "js-beautify": "^1.0",
    "ts-jest": "^22.0",
    "tslint": "^5.0",
    "typescript": "^2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts)"
    ]
  }
}
